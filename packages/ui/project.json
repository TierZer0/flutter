{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "ui",
  "sourceRoot": "packages/ui/src",
  "projectType": "library",
  "targets": {
    "analyze": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "packages/ui"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "packages/ui"
      }
    },
    "format": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter format packages/ui/*",
        "cwd": "packages/ui"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter test",
        "cwd": "packages/ui"
      }
    },
    "doctor": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter doctor",
        "cwd": "packages/ui"
      }
    },
    "build-aar": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build aar",
        "cwd": "packages/ui"
      },
      "outputs": [
        "packages/ui/build"
      ]
    },
    "build-apk": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build apk",
        "cwd": "packages/ui"
      },
      "outputs": [
        "packages/ui/build"
      ]
    },
    "build-appbundle": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build appbundle",
        "cwd": "packages/ui"
      },
      "outputs": [
        "packages/ui/build"
      ]
    },
    "build-bundle": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build bundle",
        "cwd": "packages/ui"
      },
      "outputs": [
        "packages/ui/build"
      ]
    },
    "build-ios": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ios",
        "cwd": "packages/ui"
      },
      "outputs": [
        "packages/ui/build"
      ]
    },
    "build-ios-framework": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ios-framework",
        "cwd": "packages/ui"
      },
      "outputs": [
        "packages/ui/build"
      ]
    },
    "build-ipa": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ipa",
        "cwd": "packages/ui"
      },
      "outputs": [
        "packages/ui/build"
      ]
    }
  },
  "tags": []
}
